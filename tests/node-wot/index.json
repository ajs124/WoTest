[
  {
    "path": "producer_http_plain_empty.js",
    "type": 0,
    "timeoutSec": 10,
    "protocolTestProperties": {
      "mode": 0,
      "protocol": 0,
      "requestUrl": "http://127.0.0.1:8080",
      "mustMatch": [
        "http_plain_empty ready"
      ]
    }
  },
  {
    "path": "producer_https_plain_empty.js",
    "type": 0,
    "timeoutSec": 10,
    "protocolTestProperties": {
      "mode": 0,
      "protocol": 1,
      "requestUrl": "https://127.0.0.1:8080",
      "mustMatch": [
        "https_plain_empty ready"
      ]
    }
  },
  {
    "path": "producer_coap_plain_empty.js",
    "type": 0,
    "timeoutSec": 10,
    "protocolTestProperties": {
      "mode": 0,
      "protocol": 2,
      "requestUrl": "coap://127.0.0.1",
      "mustMatch": [
        "coap_plain_empty ready"
      ]
    }
  },
  {
    "path": "consumer_http.js",
    "type": 0,
    "timeoutSec": 10,
    "protocolTestProperties": {
      "mode": 1,
      "protocol": 0,
      "serveAt": "127.0.0.1:8081",
      "serveContent": "./empty_td.json",
      "mustMatch": [
          "received Content-Type: application/json",
          "consuming TD"
      ]
    }
  },
  {
    "path": "consumer_https.js",
    "type": 0,
    "timeoutSec": 10,
    "protocolTestProperties": {
      "mode": 1,
      "protocol": 1,
      "serveAt": "127.0.0.1:8081",
      "serveContent": "./empty_td.json",
      "tlsKey": "../tests/rsa.key",
      "tlsCert": "../tests/rsa.crt",
      "mustMatch": [
          "received Content-Type: application/json",
          "consuming TD"
      ]
    }
  },

  {
    "path": "consumer_coap.js",
    "type": 0,
    "timeoutSec": 10,
    "protocolTestProperties": {
      "mode": 1,
      "protocol": 2,
      "serveAt": "127.0.0.1:5683",
      "serveContent": "./empty_td.json",
      "mustMatch": [
          "received Content-Format: application/json",
          "consuming TD"
      ]
    }
  }
]
